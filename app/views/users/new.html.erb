<%= form_for @user, html: { multipart: true } do |f| %>
	<div class="field">
    <%= f.label :avatar %><br>
    <%= f.file_field :avatar %>
    <%=f.label :email %>
    <%= f.submit :update %>

		<%=f.text_field :email %>

		<%=f.label :password %>

		<%=f.password_field :password %>

		<%=f.submit "Sign Up" %>

  </div>
<% end %>